@model IEnumerable<InternTest.Models.Product>
@{
    ViewBag.Title = "Home Page";
    //Layout = null;

}
<br />
<div id="testPage">
    <table class="table table-striped table-condensed" id="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Office</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Office</td>
                </tr>

            }
        </tbody>
    </table>
</div>

<div class="row">
    <div class="col-md-12">
        @{
            Double page = ViewBag.Totalpage;
            for (int i = 1; i <= page; i++)
            {
                <ul class="pagination">
                    <li>
                        @*<a>@i  </a>*@
                        @*@Html.ActionLink(i.ToString(), "Index", new { id = i })*@
                        <a class="btn btn-primary " onclick="pageid('@Url.Action("IndexLite","Home", new { id = i })')">@i</a>
                    </li>
                </ul>

            }

        }

    </div>
</div>
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")

} 